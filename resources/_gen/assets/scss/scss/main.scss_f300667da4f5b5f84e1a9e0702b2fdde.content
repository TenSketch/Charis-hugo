@import url("https://fonts.googleapis.com/css2?family=Petit+Formal+Script&display=swap");
@import url("https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,200;0,300;0,400;0,500;0,700;1,200;1,300;1,400&display=swap");
/* universal selector */
*,
*::before,
*::after {
  box-sizing: border-box;
  margin: 0;
  padding: 0; }

html {
  scroll-behavior: smooth;
  font-size: 14px; }

body {
  font-family: "Poppins", sans-serif;
  font-weight: 300;
  line-height: 1.5;
  overflow-x: hidden; }

a {
  text-decoration: none;
  color: #352c1c;
  transition: opacity 250ms ease-in; }

a:hover {
  opacity: 0.85; }

ul {
  list-style: none; }

img {
  max-width: 100%;
  height: auto;
  display: block; }

h1,
h2 {
  font-weight: 300; }

section {
  padding-block: 8rem; }

.container {
  max-width: 90%;
  width: 1200px;
  margin: 0 auto;
  position: relative; }

.txt-center, .testimonials .container h2, .products .container h2 {
  text-align: center; }

.flex-col, .products .container .card .card-body, .products .container .card {
  flex-direction: column; }

.flex-wrap {
  flex-wrap: wrap; }

.gap-1, .products .container {
  gap: 1em; }

.grid {
  display: grid;
  place-items: center; }

.btn {
  display: inline-block;
  padding: .5em 1em;
  font-size: 1.1rem;
  margin-top: .7em;
  background: #352c1c;
  color: #f8f5f2;
  border: none; }

.my-1, .content h2, .content h3, .content p, .footer li, .testimonials .container .testimonials_card h4, .advantage .content ul li {
  margin-block: 1em; }

.mt-1 {
  margin-top: 1em; }

.mt-2 {
  margin-top: 2em; }

.mt-3 {
  margin-top: 3em; }

.mt-4, .content {
  margin-top: 4em; }

.p-1 {
  padding: 1em; }

/* navbar */
header {
  padding: 1em;
  background: #fff;
  box-shadow: 0 4px 12px 0 rgba(31, 38, 135, 0.37);
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 10; }
  header .container {
    display: flex;
    align-items: center;
    justify-content: normal;
    flex-wrap: nowrap;
    width: 1000px;
    max-width: 90%; }
  header .logo {
    flex-basis: 10%; }
    header .logo img {
      min-width: 7rem; }
  header .svg-inline--fa {
    max-width: 30px;
    aspect-ratio: 1;
    width: 17px;
    display: block; }
  header nav {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: normal;
    flex-wrap: nowrap; }
    header nav a {
      color: #9b8055;
      text-decoration: none; }
    header nav .nav_list {
      position: absolute;
      top: -1000%;
      right: 0;
      width: 50% !important;
      background: #fff;
      padding: 1em; }
      header nav .nav_list li {
        position: relative;
        margin-block: 1.5em;
        text-align: right; }
        @media (min-width: 991px) {
          header nav .nav_list li {
            text-align: unset;
            margin-block: 0; } }
    header nav .nav_list.active {
      top: 10vh;
      transition: all 400ms ease; }
    header nav .nav_icons {
      gap: 1em;
      margin-left: auto;
      margin-right: 2.5em;
      display: flex;
      align-items: center;
      justify-content: normal;
      flex-wrap: nowrap; }
      @media (min-width: 991px) {
        header nav .nav_icons {
          margin-right: 0; } }
      header nav .nav_icons img {
        display: unset; }
      header nav .nav_icons span {
        display: none; }
      header nav .nav_icons .cart a {
        display: flex; }
      header nav .nav_icons .cart .price {
        display: none; }
      header nav .nav_icons .cart:hover .price {
        display: block; }
      header nav .nav_icons .nav_dropdown .dropdown-menu {
        display: none;
        position: absolute;
        background-color: #f8f5f2;
        min-width: 158px;
        z-index: 1; }
        header nav .nav_icons .nav_dropdown .dropdown-menu a {
          text-decoration: none;
          color: #9b8055;
          display: flex;
          flex-direction: column;
          padding: 1em; }
      header nav .nav_icons .dropdown-menu a:hover {
        background-color: #9b8055;
        color: #f8f5f2; }
      header nav .nav_icons .nav_dropdown:hover .dropdown-menu {
        display: block;
        background-color: #fff; }
    @media (min-width: 991px) {
      header nav .nav_list {
        margin-left: 1em;
        position: relative;
        display: flex;
        align-items: center;
        justify-content: flex-start;
        flex-wrap: nowrap;
        margin-block: 0;
        gap: 1.5em; }
      header nav .nav_icons > div a {
        display: flex;
        align-items: flex-start;
        justify-content: normal;
        flex-wrap: nowrap;
        gap: .5em; }
      header nav .nav_icons span {
        display: inline-block; } }
  header .nav__toggle {
    display: block;
    position: absolute;
    right: 0;
    width: 25px;
    height: 25px;
    cursor: pointer;
    z-index: 3;
    background: none;
    color: inherit;
    border: none;
    transition: all 400ms ease; }
    header .nav__toggle .bar1,
    header .nav__toggle .bar2,
    header .nav__toggle .bar3 {
      display: block;
      background: #9b8055;
      height: 3px;
      margin: 3px auto;
      transition: 0.4s; }
    header .nav__toggle .bar1.change {
      transform: rotate(-70deg) translate(-9px, -5px);
      width: 70%; }
    header .nav__toggle .bar2.change {
      transform: translateY(10px); }
    header .nav__toggle .bar3.change {
      transform: rotate(70deg) translate(-2px, -9px);
      width: 70%; }
    @media (min-width: 991px) {
      header .nav__toggle {
        display: none; } }
  header .nav__toggle.active {
    position: fixed;
    right: 20px; }

.banner {
  background-color: #f8f5f2;
  text-align: center; }
  .banner .image img {
    filter: drop-shadow(0 15px 5px #352c1c);
    max-width: 60%;
    margin: auto; }

.advantage {
  color: #f8f5f2;
  background: rgba(33, 27, 18, 0.7) url(/images/Women_and_Style/Shine/Shine-womens-Jute-bags-5.jpg) center fixed no-repeat;
  background-size: cover;
  background-blend-mode: multiply; }
  .advantage .content ul li {
    position: relative;
    padding-left: 1.5em; }
    .advantage .content ul li::before {
      position: absolute;
      content: '';
      background: url(/images/svg/hand-point-right-regular.svg) center no-repeat;
      background-size: cover;
      top: 0;
      left: 0;
      display: inline-block;
      height: 1.2em;
      aspect-ratio: 1;
      filter: invert(1); }

.products .container {
  display: flex;
  align-items: normal;
  justify-content: space-around;
  flex-wrap: wrap; }
  .products .container h2 {
    flex-basis: 100%;
    font-size: 2rem; }
  .products .container .card {
    display: flex;
    align-items: normal;
    justify-content: space-between;
    flex-wrap: nowrap;
    flex-basis: 40%; }
    @media (min-width: 768px) {
      .products .container .card {
        flex-basis: 30%; } }
    @media (min-width: 991px) {
      .products .container .card {
        flex-basis: 15%; } }
    .products .container .card .card-head {
      flex-basis: 40%;
      align-self: center; }
      .products .container .card .card-head a {
        height: 100%;
        margin: auto;
        display: flex;
        align-items: flex-end; }
    .products .container .card .card-body {
      min-height: 15rem;
      flex-basis: 40%;
      display: flex;
      align-items: normal;
      justify-content: space-between;
      flex-wrap: nowrap; }
      .products .container .card .card-body .card-title {
        height: 100%;
        display: grid;
        place-items: center; }
        .products .container .card .card-body .card-title a {
          font-size: 1.3rem;
          color: #211b12; }
      .products .container .card .card-body .card-desc {
        height: 100%;
        display: grid;
        place-items: center;
        font-family: "Petit Formal Script", cursive; }
      .products .container .card .card-body .price s {
        font-size: smaller; }
      .products .container .card .card-body .price span {
        font-weight: 600; }

@media (min-width: 991px) {
  .about .container {
    display: flex;
    align-items: center;
    justify-content: space-between;
    flex-wrap: nowrap; } }

.about .about_text {
  width: 100%;
  padding: 4rem;
  background-color: rgba(248, 245, 242, 0.8);
  border-radius: 8px;
  color: #352c1c; }

.about .btn {
  display: inline-block;
  background-color: #352c1c;
  color: #f8f5f2;
  padding: .7em 1em;
  border-radius: 25px; }

@media (min-width: 900px) {
  .about .container_about {
    flex-direction: row;
    align-items: center;
    margin-top: 10%; }
  .about .about_text {
    background-color: unset;
    color: #352c1c; }
    .about .about_text h3 {
      font-weight: 400; } }

.testimonials {
  background-color: #c5b396; }
  .testimonials .container {
    /* flex-direction: column; */
    /* justify-content: space-around; */
    display: flex;
    align-items: normal;
    justify-content: center;
    flex-wrap: nowrap;
    gap: 2.5em;
    flex-wrap: wrap;
    margin-block: 2em; }
    .testimonials .container h2 {
      flex-basis: 100%; }
    .testimonials .container .testimonials_card {
