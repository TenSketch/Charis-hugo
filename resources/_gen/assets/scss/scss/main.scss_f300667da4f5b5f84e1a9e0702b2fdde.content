.footer {
  background-color: #352c1c;
  color: #f8f5f2; }
  @media (min-width: 900px) {
    .footer .container {
      display: flex;
      align-items: flex-start;
      justify-content: space-between;
      flex-wrap: nowrap; } }
.col-1 li a {
  color: #f8f5f2; }

.footer h3 {
  font-size: 1.5rem;
  margin-bottom: 1em; }

.social-links a {
  width: 32px;
  margin: 0 0.2rem;
  text-decoration: none;
  font-size: 2rem;
  transition: all 0.5s ease;
  color: white; }
  .social-links a:hover {
    background: #000;
    border-color: transparent;
    color: #fff; }

@media (min-width: 700px) {
  .footer {
    flex-direction: row; }
  .col {
