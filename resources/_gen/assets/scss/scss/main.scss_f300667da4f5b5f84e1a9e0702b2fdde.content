/* navbar */
header {
  width: 100%;
  background-color: #f8f5f2;
  position: relative; }
  header .container {
    display: flex;
    align-items: center;
    justify-content: space-around;
    flex-wrap: nowrap;
    position: static; }
    header .container .logo {
      flex: 1; }
      header .container .logo img {
        min-width: 8rem;
        width: 10rem; }
    header .container .cart {
      transition: display 250ms ease-in; }
      header .container .cart .price {
        display: none; }
      header .container .cart:hover .price {
        display: block; }

.nav_list {
  flex: 2;
  position: absolute;
  width: 100%;
  right: 0;
  top: -1000vh;
  z-index: 3;
  max-width: 100%;
  transition: all 400ms ease-in-out; }
  @media (min-width: 991px) {
    .nav_list {
      width: auto;
      height: auto;
      right: inherit;
      top: 50%;
      transform: translateY(-50%); } }
  .nav_list .nav_list--item {
    margin-block: 3em; }
    @media (min-width: 991px) {
      .nav_list .nav_list--item {
        margin-block: auto;
        display: inline-block; }
        .nav_list .nav_list--item + .nav_list--item {
          margin-left: 3.5rem; } }
  .nav_list.active {
    top: 100%;
    transition: all 400ms ease-in-out;
    background-color: #f8f5f2; }
  .nav_list .nav_list--item {
    font-size: 1.2rem;
    margin-left: 2rem; }

.dropdown {
  position: relative;
  transition: 250ms all ease-in-out; }
  .dropdown .dropdown-toggle {
    position: relative; }
    .dropdown .dropdown-toggle img {
      max-width: 1.2rem;
      display: unset;
      position: absolute;
      bottom: 0;
      margin-left: .5rem; }
  .dropdown .dropdown-menu {
    display: none;
    position: absolute;
    background-color: #f8f5f2;
    min-width: 160px;
    box-shadow: 0px 8px 16px 0px rgba(0, 0, 0, 0.2);
    z-index: 1; }
    .dropdown .dropdown-menu .nav_link {
      color: black;
      padding: 12px 16px;
      text-decoration: none;
      display: block; }
      .dropdown .dropdown-menu .nav_link:hover {
        background-color: #ddd; }
  .dropdown:hover .dropdown-menu {
    display: block; }

.nav_icons {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  flex-wrap: nowrap; }

.svg-inline--fa {
  max-width: 30px;
  width: 17px;
  display: unset; }

.nav__toggle {
  display: block;
  position: absolute;
  left: 2em;
  top: 100%;
  width: 25px;
  height: 25px;
  cursor: pointer;
  z-index: 3;
  background: none;
  color: inherit;
  border: none;
  transition: all 400ms ease; }
  .nav__toggle .bar1,
  .nav__toggle .bar2,
  .nav__toggle .bar3 {
    display: block;
    background: #0e1b43;
    height: 3px;
    margin: 3px auto;
    transition: 0.4s; }
  .nav__toggle .bar1.change {
    transform: rotate(-75deg) translate(-10px, -6px);
    width: 70%; }
  .nav__toggle .bar2.change {
    transform: translateY(10px); }
  .nav__toggle .bar3.change {
    transform: rotate(75deg) translate(-2px, -10px);
    width: 70%; }
  @media (min-width: 991px) {
    .nav__toggle {
      display: none; } }
